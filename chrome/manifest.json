{
  "name": "FYDP WebGL Extension",
  "description": "The best WebGL extension ever!",
  "manifest_version": 2,
  "version": "0.1",
  "web_accessible_resources": ["main.js"],
  "content_scripts": [
    {
      "run_at": "document_start",
      "matches": ["<all_urls>"],
      "all_frames": true,
      "js": ["external/jquery.js", "bind.js"]
    }
  ]
}